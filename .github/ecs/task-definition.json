{
  "family": "brev-ly-ecs-task-definition",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::842676008268:role/brev-ly-ecs-task-execution-role",
  "containerDefinitions": [
    {
      "name": "brev-ly-ecs-task-definition",
      "image": "ghcr.io/gabrielmatsan/brev-ly:latest",
      "repositoryCredentials": {
        "credentialsParameter": "arn:aws:secretsmanager:us-east-1:842676008268:secret:brev-ly-github-credentials-AzwmlC"
      },
      "cpu": 512,
      "memory": 1024,
      "memoryReservation": 1024,
      "portMappings": [
        {
          "name": "brev-ly-8080-tcp",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "stopTimeout": 30,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ],
      "secrets": [
        {
          "name": "DATAPORT",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:842676008268:secret:brev-ly-github-credentials-AzwmlC:dataport::"
        },
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:842676008268:secret:brev-ly-github-credentials-AzwmlC:database_url::"
        },
        {
          "name": "FRONTEND_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:842676008268:secret:brev-ly-github-credentials-AzwmlC:frontend_url::"
        },
        {
          "name": "CLOUDFLARE_ACCOUNT_ID",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:842676008268:secret:brev-ly-github-credentials-AzwmlC:cloudflare_account_id::"
        },
        {
          "name": "CLOUDFLARE_ACCESS_KEY_ID",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:842676008268:secret:brev-ly-github-credentials-AzwmlC:cloudflare_access_key_id::"
        },
        {
          "name": "CLOUDFLARE_SECRET_ACCESS_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:842676008268:secret:brev-ly-github-credentials-AzwmlC:cloudflare_secret_access_key::"
        },
        {
          "name": "CLOUDFLARE_BUCKET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:842676008268:secret:brev-ly-github-credentials-AzwmlC:cloudflare_bucket::"
        },
        {
          "name": "CLOUDFLARE_PUBLIC_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:842676008268:secret:brev-ly-github-credentials-AzwmlC:cloudflare_public_url::"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "systemControls": [],
      "healthCheck": {
        "command": [
          "CMD",
          "node",
          "healthcheck.js"
        ],
        "interval": 30,
        "timeout": 10,
        "retries": 3,
        "startPeriod": 60
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/brev-ly",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "volumes": [],
  "tags": [
    {
      "key": "Environment",
      "value": "production"
    },
    {
      "key": "Name",
      "value": "brev-ly-ecs-task-definition"
    },
    {
      "key": "Service",
      "value": "api"
    }
  ]
}